package localandgobalveriables;

public class sample {
	
	int a=10, b;
	
	public static void main(String[] args) {
		
		int a =15, b;
		System.out.println("local variable value is "+a);
	
	}
    public static void printvalue(int a) {
    	System.out.println("gobal variable value is "+a);
    }
    

}
